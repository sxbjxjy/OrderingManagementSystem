
@model List<OrderingManagementSystem.Models.Employee>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        @for (int i = 0; i < Model.Count(); i++)
        {
            @Html.DisplayFor(m => m[i].EmpNo)
            @Html.DisplayFor(m => m[i].EmpName)
        }
    </div>
</body>
</html>
